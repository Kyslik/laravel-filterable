{
    "name": "kivicms/laravel-filterable",
    "description": "Using URL query strings to filter Eloquent queries.",
    "keywords": [
        "kyslik",
        "laravel-filterable",
        "laravel",
        "filter",
        "query-strings"
    ],
    "homepage": "https://github.com/kivicms/laravel-filterable",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Kiesel",
            "email": "martin.kiesel@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Vitaliy Ivanov",
            "email": "ivanovv1972@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^8.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "KiviCms\\LaravelFilterable\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KiviCms\\LaravelFilterable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "KiviCms\\LaravelFilterable\\FilterableServiceProvider"
            ]
        }
    }
}
